{
  "puppet_task_version": 1,
  "supports_noop": false,
  "description": "Execute the entire Ansible role imperatively",
  "input_method": "environment",
  "parameters": {
    "kubernetes_version": {
      "description": "Ansible role variable (default: 1.33)",
      "type": "Optional[String]"
    },
    "kubernetes_config_kubeadm_apiversion": {
      "description": "Ansible role variable (default: v1beta4)",
      "type": "Optional[String]"
    },
    "kubenetes_config_kubelet_apiversion": {
      "description": "Ansible role variable (default: v1beta1)",
      "type": "Optional[String]"
    },
    "kubernetes_config_kubeproxy_apiversion": {
      "description": "Ansible role variable (default: v1alpha1)",
      "type": "Optional[String]"
    },
    "kubernetes_packages": {
      "description": "Ansible role variable (default: [{\"name\"=>\"kubelet\", \"state\"=>\"present\"}, {\"name\"=>\"kubectl\", \"state\"=>\"present\"}, {\"name\"=>\"kubeadm\", \"state\"=>\"present\"}, {\"name\"=>\"kubernetes-cni\", \"state\"=>\"present\"}])",
      "type": "Optional[Array]"
    },
    "kubernetes_version_rhel_package": {
      "description": "Ansible role variable (default: 1.33)",
      "type": "Optional[String]"
    },
    "kubernetes_role": {
      "description": "Ansible role variable (default: control_plane)",
      "type": "Optional[String]"
    },
    "kubernetes_kubelet_extra_args": {
      "description": "This is deprecated. Please use kubernetes_config_kubelet_configuration instead.",
      "type": "Optional[String]"
    },
    "kubernetes_kubeadm_init_extra_opts": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "kubernetes_join_command_extra_opts": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "kubernetes_allow_pods_on_control_plane": {
      "description": "Ansible role variable (default: true)",
      "type": "Optional[Boolean]"
    },
    "kubernetes_pod_network": {
      "description": "Ansible role variable (default: {\"cni\"=>\"flannel\", \"cidr\"=>\"10.244.0.0/16\"})",
      "type": "Optional[Hash]"
    },
    "kubernetes_kubeadm_kubelet_config_file_path": {
      "description": "Ansible role variable (default: /etc/kubernetes/kubeadm-kubelet-config.yaml)",
      "type": "Optional[String]"
    },
    "kubernetes_config_kubelet_configuration": {
      "description": "Ansible role variable (default: {\"cgroupDriver\"=>\"systemd\"})",
      "type": "Optional[Hash]"
    },
    "kubernetes_config_init_configuration": {
      "description": "Ansible role variable (default: {\"localAPIEndpoint\"=>{\"advertiseAddress\"=>\"{{ kubernetes_apiserver_advertise_address | default(ansible_default_ipv4.address, true) }}\"}})",
      "type": "Optional[Hash]"
    },
    "kubernetes_config_cluster_configuration": {
      "description": "Ansible role variable (default: {\"networking\"=>{\"podSubnet\"=>\"{{ kubernetes_pod_network.cidr }}\"}, \"kubernetesVersion\"=>\"{{ kubernetes_version_kubeadm }}\"})",
      "type": "Optional[Hash]"
    },
    "kubernetes_config_kube_proxy_configuration": {
      "description": "Ansible role variable (default: {})",
      "type": "Optional[Hash]"
    },
    "kubernetes_apiserver_advertise_address": {
      "description": "Ansible role variable (default: )",
      "type": "Optional[String]"
    },
    "kubernetes_version_kubeadm": {
      "description": "Ansible role variable (default: stable-{{ kubernetes_version }})",
      "type": "Optional[String]"
    },
    "kubernetes_ignore_preflight_errors": {
      "description": "Ansible role variable (default: all)",
      "type": "Optional[String]"
    },
    "kubernetes_apt_release_channel": {
      "description": "Ansible role variable (default: stable)",
      "type": "Optional[String]"
    },
    "kubernetes_apt_repository": {
      "description": "Ansible role variable (default: https://pkgs.k8s.io/core:/{{ kubernetes_apt_release_channel }}:/v{{ kubernetes_version }}/deb/)",
      "type": "Optional[String]"
    },
    "kubernetes_yum_base_url": {
      "description": "Ansible role variable (default: https://pkgs.k8s.io/core:/stable:/v{{ kubernetes_version }}/rpm/)",
      "type": "Optional[String]"
    },
    "kubernetes_yum_gpg_key": {
      "description": "Ansible role variable (default: https://pkgs.k8s.io/core:/stable:/v{{ kubernetes_version }}/rpm/repodata/repomd.xml.key)",
      "type": "Optional[String]"
    },
    "kubernetes_yum_gpg_check": {
      "description": "Ansible role variable (default: true)",
      "type": "Optional[Boolean]"
    },
    "kubernetes_yum_repo_gpg_check": {
      "description": "Ansible role variable (default: true)",
      "type": "Optional[Boolean]"
    },
    "kubernetes_flannel_manifest_file": {
      "description": "Flannel config file.",
      "type": "Optional[String]"
    },
    "kubernetes_calico_manifest_file": {
      "description": "Calico config file.",
      "type": "Optional[String]"
    },
    "par_tags": {
      "description": "An array of Ansible tags to execute (optional)",
      "type": "Optional[Array[String]]"
    },
    "par_skip_tags": {
      "description": "An array of Ansible tags to skip (optional)",
      "type": "Optional[Array[String]]"
    },
    "par_start_at_task": {
      "description": "The name of the task to start execution at (optional)",
      "type": "Optional[String]"
    },
    "par_limit": {
      "description": "Limit playbook execution to specific hosts (optional)",
      "type": "Optional[String]"
    },
    "par_verbose": {
      "description": "Enable verbose output from Ansible (optional)",
      "type": "Optional[Boolean]"
    },
    "par_check_mode": {
      "description": "Run Ansible in check mode (dry-run) (optional)",
      "type": "Optional[Boolean]"
    },
    "par_timeout": {
      "description": "Timeout in seconds for playbook execution (optional)",
      "type": "Optional[Integer]"
    },
    "par_user": {
      "description": "Remote user to use for Ansible connections (optional)",
      "type": "Optional[String]"
    },
    "par_env_vars": {
      "description": "Additional environment variables for ansible-playbook execution (optional)",
      "type": "Optional[Hash]"
    }
  },
  "implementations": [
    {
      "name": "paw_ansible_role_kubernetes.sh",
      "requirements": [
        "shell"
      ],
      "input_method": "environment"
    },
    {
      "name": "paw_ansible_role_kubernetes.ps1",
      "requirements": [
        "powershell"
      ],
      "input_method": "environment"
    }
  ]
}